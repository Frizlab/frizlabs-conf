[user]
	name = Frizlab
	email = fload@me.com
[push]
	default = current
[diff]
	tool = opendiff
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
[core]
	excludesfile = ~/.gitignore_global
[alias]
	last = log -1 HEAD
	fa = fetch -pPtf
	pr = pull --rebase
	pushf = push --force-with-lease
[init]
	defaultBranch = main
[checkout]
	defaultRemote = origin
